#include <stdio.h>
#include <string.h>

struct Customer {
    int id;
    char name[30];
    char mobile[15];
};

int main() {
    struct Customer c[3];
    int i, sid;

    for(i = 0; i < 3; i++) {
        scanf("%d %s %s", &c[i].id, c[i].name, c[i].mobile);
    }

    printf("Enter ID to search: ");
    scanf("%d", &sid);

    for(i = 0; i < 3; i++) {
        if(c[i].id == sid) {
            printf("Name:%s Mobile:%s", c[i].name, c[i].mobile);
            return 0;
        }
    }
    printf("Customer not found");
    return 0;
}
