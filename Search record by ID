#include <stdio.h>

struct Emp {
    int id;
    char name[20];
};

int main() {
    struct Emp e[3];
    int i, sid;

    for(i = 0; i < 3; i++) {
        scanf("%d %s", &e[i].id, e[i].name);
    }

    scanf("%d", &sid);

    for(i = 0; i < 3; i++) {
        if(e[i].id == sid) {
            printf("Name: %s", e[i].name);
            return 0;
        }
    }
    printf("Not Found");
    return 0;
}
