#include <stdio.h>

struct Emp {
    int id;
    char name[20];
};

int main() {
    struct Emp e[3];
    int i, uid;

    for(i = 0; i < 3; i++) {
        scanf("%d %s", &e[i].id, e[i].name);
    }

    scanf("%d", &uid);

    for(i = 0; i < 3; i++) {
        if(e[i].id == uid) {
            scanf("%s", e[i].name);
            printf("Updated Name: %s", e[i].name);
            return 0;
        }
    }
    return 0;
}
